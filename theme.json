{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
    "appearanceTools": true,
    "color": {
      "defaultPalette": false,
      "palette": [
        { "slug": "u-white", "color": "#fdfbee", "name": "udemy tut white" },
        {
          "slug": "u-gray-100",
          "color": "rgb(243 244 246)",
          "name": "Udemy Gray 100"
        },
        {
          "slug": "u-gray-200",
          "color": "rgb(229 231 235)",
          "name": "Udemy Gray 200"
        },
        {
          "slug": "u-gray-300",
          "color": "rgb(209 213 219)",
          "name": "Udemy Gray 300"
        },
        {
          "slug": "u-gray-400",
          "color": "rgb(156 163 175)",
          "name": "Udemy Gray 400"
        },
        {
          "slug": "u-gray-500",
          "color": "rgb(107 114 128)",
          "name": "Udemy Gray 500"
        },
        {
          "slug": "u-gray-600",
          "color": "rgb(75 85 99)",
          "name": "Udemy Gray 600"
        },
        {
          "slug": "u-gray-700",
          "color": "#555152",
          "name": "Udemy Gray 700"
        },
        {
          "slug": "u-gray-800",
          "color": "#45484b",
          "name": "Udemy Gray 800"
        },
        {
          "slug": "u-gray-900",
          "color": "rgb(17 24 39)",
          "name": "#36393b"
        },
        {
          "slug": "u-primary",
          "color": "#49708a",
          "name": "Udemy Primary"
        },
        {
          "slug": "u-accent",
          "color": "#fd6081",
          "name": "Udemy Accent"
        },
        {
          "slug": "u-success",
          "color": "#259073",
          "name": "Udemy Success"
        },
        { "slug": "u-info", "color": "#32b9be", "name": "Udemy Info" },
        {
          "slug": "u-warning",
          "color": "#b3c631",
          "name": "Udemy Warning"
        },
        {
          "slug": "u-danger",
          "color": "#941f1f",
          "name": "Udemy Danger"
        }
      ],
      "duotone": [
        {
          "colors": ["#0b2e59", "#a7dbd8"],
          "slug": "dark-blue-and-light-blue",
          "name": "Dark blue and light blue"
        },
        {
          "colors": ["#0b2e59", "#e5f04c"],
          "slug": "dark-blue-and-greeen",
          "name": "Dark blue and greeen"
        }
      ],
      "gradients": [
        {
          "gradient": "linear-gradient(#cbe86b, #1b325f)",
          "name": "udemy-gradient",
          "slug": "u-gradient"
        }
      ],
      "defaultGradients": false,
      "background": true,
      "text": true,
      "button": true,
      "link": true
    },
    "typography": {
      "fontFamilies": [
        {
          "fontFamily": "Roboto Slab,sanserif",
          "slug": "u-roboto-slab",
          "name": "Udemy Roboto Slab"
        },
        {
          "fontFamily": "Lemon,sanserif",
          "slug": "u-lemon",
          "name": "Udemy Lemon"
        }
      ],
      "fontSizes": [
        { "slug": "small", "fluid": false, "size": ".75rem", "name": "small" },
        { "slug": "medium", "fluid": false, "size": "1rem", "name": "medium" },
        { "slug": "large", "fluid": false, "size": "1.75rem", "name": "large" },
        {
          "slug": "extra-large",
          "fluid": {
            "min": "2rem",
            "max": "2.5rem"
          },
          "size": "2.5rem",
          "name": "extra-large"
        },
        {
          "slug": "ginormous",
          "fluid": false,
          "size": "3.5rem",
          "name": "ginormous"
        }
      ],
      "lineHeight": true,
      "fontStyle": true,
      "fontWeight": true,
      "textTransform": true,
      "letterSpacing": true,
      "textDecoration": true,
      "dropCap": true
    },
    "blocks": {
      "core/site-title": {
        "color": {
          "defaultPalette": true,
          "palette": [
            {
              "slug": "u-primary-mine",
              "color": "#601848",
              "name": "udemy maroon"
            }
          ],
          "link": true
        },
        "typography": {
          "fontFamily": "var(--wp--preset--font-family--u-lemon)"
        }
      }
    },
    "core/preformatted": {},
    "border": {
      "color": true,
      "radius": true,
      "style": true,
      "width": true
    },
    "layout": {
      "contentSize": "50rem",
      "wideSize": "70rem"
    },
    "spacing": {
      "margin": true,
      "padding": true,
      "blockGap": true
    }
  },
  "styles": {
    "color": {
      "text": "var(--wp--preset--color--u-warning)",
      "background": "#fdfbee"
    },
    "spacing": {
      "margin": {
        "bottom": "0px",
        "left": "0px",
        "right": "0px",
        "top": "0px"
      },
      "padding": {
        "bottom": "0px",
        "left": "0px",
        "right": "0px",
        "top": "0px"
      },
      "blockGap": "4rem"
    },
    "blocks": {
      "core/button": {
        "color": {
          "gradient": "var(--wp--preset--gradient--u-gradient)"
        }
      },
      "core/pullquote": {
        "border": {
          "width": "1px",
          "radius": "6px",
          "style": "solid",
          "color": "var(--wp--preset--color--u-success)"
        }
      }
    },
    "elements": {
      "link": {
        "color": {
          "text": "var(--wp--preset--color--u-success)"
        }
      }
    },
    "typography": {
      "fontFamily": "var(--wp--preset--font-family--u-roboto-slab)"
    }
  }
}
